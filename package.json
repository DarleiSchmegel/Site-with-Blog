{
  "name": "site-with-blog",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check-lint": "next lint",
    "generate": "graphql-codegen --config codegen.yml",
    "check-prettier": "prettier --config .prettierrc --check \"**/*.{js,jsx}\"",
    "prepare": "husky install",
    "checks": "yarn install --check-files && yarn run check-prettier && yarn run check-lint"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.17",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "^8.4.1",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "typescript": "4.5.2"
  }
}
